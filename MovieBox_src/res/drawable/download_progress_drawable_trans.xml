<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/white_20" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/brand_new_gradient_center" android:endColor="@color/brand_new_gradient_end" android:angle="0.0" android:centerColor="@color/brand_new_gradient_start" />
                <corners android:radius="2.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>