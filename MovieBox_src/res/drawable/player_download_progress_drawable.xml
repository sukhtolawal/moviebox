<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#ffe4e6eb" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff336fff" android:endColor="#ff00e069" android:angle="0.0" android:centerColor="#ff00e069" />
                <corners android:radius="2.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>