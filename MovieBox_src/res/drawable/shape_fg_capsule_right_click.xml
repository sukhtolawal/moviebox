<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/mini_dp_0_5" android:color="@color/mini_color_transparent" />
            <corners android:topRightRadius="@dimen/mini_dp_16" android:bottomRightRadius="@dimen/mini_dp_16" />
            <solid android:color="@color/mini_color_33191f2b" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/mini_dp_0_5" android:color="@color/mini_color_transparent" />
            <corners android:topRightRadius="@dimen/mini_dp_16" android:bottomRightRadius="@dimen/mini_dp_16" />
            <solid android:color="@color/mini_color_33191f2b" />
        </shape>
    </item>
</selector>